{
  "name": "wpamvueapp",
  "version": "0.0.1",
  "description": "A Vue JS Admin App for WordPress - Webpack, Sass, Vue, Vuex, Vue-router",
  "main": "src/main.js",
  "scripts": {
    "dev": "WEBPACK_ENV=development webpack --progress --watch --config ./webpack/webpack.config.js",
    "compile": "clear && webpack --progress --mode production --config ./webpack/webpack.config.js",
    "build": "WEBPACK_ENV=production webpack --config ./webpack/webpack.config.js",
    "translate": "wp i18n make-pot ./ languages/wp-am-vue-app.pot --exclude=builds/,node_modules/,src,builds --ignore-domain --allow-root",
    "build:zip": "clear && grunt preBuildClean && npm run compile && npm run translate && grunt copy:pro && grunt compress:pro && grunt finish",
    "lint": "eslint --config .eslintrc.js --ext .js,.vue .",
    "lint-fix": "npm run lint -- --fix"
  },
  "author": "",
  "license": "GPL v3 or later",
  "dependencies": {
    "axios": "^1.1.3",
    "chart.js": "^4.4.0",
    "vue": "^3.2.41",
    "vue-chartjs": "^5.2.0",
    "vue-router": "^4.1.5",
    "vue-toast-notification": "^3.1.2",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@wordpress/eslint-plugin": "^13.4.0",
    "@wordpress/hooks": "^3.20.0",
    "@wordpress/i18n": "^4.20.0",
    "babel-loader": "^8.2.5",
    "browser-sync-webpack-plugin": "^2.3.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.26.0",
    "eslint-plugin-vue": "^9.6.0",
    "grunt": "^1.6.1",
    "grunt-checktextdomain": "^1.0.1",
    "grunt-contrib-clean": "^2.0.1",
    "grunt-contrib-compress": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-search": "^0.1.8",
    "grunt-shell": "^4.0.0",
    "grunt-wp-i18n": "^1.0.3",
    "load-grunt-tasks": "^5.1.0",
    "mini-css-extract-plugin": "^2.6.1",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "serialize-javascript": "^6.0.0",
    "style-loader": "^3.3.1",
    "vue-loader": "^17.0.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
